        <TextBox HorizontalAlignment="Left" Margin="625,487,0,0" Text="0" TextWrapping="Wrap" VerticalAlignment="Top" Width="75" TextChanged="r5_changed"/>

        <TextBox HorizontalAlignment="Left" Margin="112,556,0,0" Text="0" TextWrapping="Wrap" VerticalAlignment="Top" Width="75" TextChanged="c1_changed"/>
        <TextBox HorizontalAlignment="Left" Margin="212,556,0,0" Text="0" TextWrapping="Wrap" VerticalAlignment="Top" Width="75" TextChanged="c2_changed"/>
        <TextBox HorizontalAlignment="Left" Margin="312,556,0,0" Text="0" TextWrapping="Wrap" VerticalAlignment="Top" Width="75" TextChanged="c3_changed"/>
        <TextBox HorizontalAlignment="Left" Margin="412,556,0,0" Text="0" TextWrapping="Wrap" VerticalAlignment="Top" Width="75" TextChanged="c4_changed"/>
        <TextBox HorizontalAlignment="Left" Margin="512,556,0,0" Text="0" TextWrapping="Wrap" VerticalAlignment="Top" Width="75" TextChanged="c5_changed"/>

        <Button Content="Solve" HorizontalAlignment="Left" Margin="710,544,0,0" VerticalAlignment="Top" Height="30" Width="80" Click="Button_Click"/>

    </Grid>
</Window>
