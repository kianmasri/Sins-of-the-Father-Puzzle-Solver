<Window x:Class="rspuzzle.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:rspuzzle"
        mc:Ignorable="d"
        Title="RS Puzzle Solver" Height="650" Width="825">
    <Grid>

        <Rectangle Name="r1c1" HorizontalAlignment="Left" Height="100" Margin="100,50,0,0" Stroke="Black" VerticalAlignment="Top" Width="100"/>
